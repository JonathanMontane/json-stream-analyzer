{
  "extends": [
    "algolia/typescript",
    "plugin:import/typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "jest": true
  },
  "plugins": [
    "prettier",
    "@typescript-eslint"
  ],
  "rules": {
    "prettier/prettier": "error",
    "import/extensions": "off",
    "import/namespace": "off",
    "eslint-comments/disable-enable-pair": [
      "error",
      {
        "allowWholeFile": true
      }
    ]
  }
}
