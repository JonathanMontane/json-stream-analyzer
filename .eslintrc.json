{
  "extends": [
    "algolia",
    "algolia/jest",
    "algolia/typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "jest": true
  },
  "settings": {
    "import/extensions": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "import/parsers": {
      "@typescript-eslint/parser": [
        ".ts",
        ".tsx"
      ]
    }
  },
  "plugins": [
    "prettier",
    "@typescript-eslint"
  ],
  "rules": {
    "import/extensions": [
      "error",
      "never"
    ],
    "@typescript-eslint/explicit-function-return-type": [
      "warn"
    ]
  }
}
