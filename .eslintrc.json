{
  "extends": [
    "algolia",
    "algolia/jest",
    "algolia/typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "jest": true
  },
  "plugins": [
    "prettier",
    "@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/explicit-function-return-type": [
      "warn"
    ]
  }
}
